    <header class="header">
        <div class="container">                       
            <!-- <img class="profile-image img-responsive pull-left" src="#{@resume.photo}"  alt="#{@resume.full_name" /> -->
            <div class="profile-content pull-left">
                <h1 class="name"><%= @resume["first_name"] %> <%= @resume["last_name"] %></h1>
                <h2 class="desc">Web App Developer</h2>   
                <ul class="social list-inline">
                    <li><a href="@resume.twitter_handle"><i class="fa fa-twitter"></i></a></li>
                    <li><a href="@resume.linkedin_url"><i class="fa fa-linkedin"></i></a></li>
                    <li><a href="@resume.github_url"><i class="fa fa-github-alt"></i></a></li>                 
                </ul> 
            </div><!--//profile-->
            <a class="btn btn-cta-primary pull-right" href="#" target="_blank"><i class="fa fa-paper-plane"></i> Contact Me</a>              
        </div><!--//container-->
    </header><!--//header-->
    <div class="container sections-wrapper">
        <div class="row">
            <div class="primary col-md-8 col-sm-12 col-xs-12">
                <section class="about section">
                    <div class="section-inner">
                        <h2 class="heading">About Me</h2>
                        <div class="content">
                            <p><%= @resume["short_bio"] %></p>
                         
                        </div><!--//content-->
                    </div><!--//section-inner-->                 
                </section><!--//section-->
    
               <section class="latest section">
                    <div class="section-inner">
                        <h2 class="heading">Capstone Project</h2>
                        <div class="content">    
                                               
                            <div class="item featured text-center">
                                <h3 class="title"><%= @resume["capstones"]["name"] %></h3>
                                <div class="featured-image">
                                    <a href="http://themes.3rdwavemedia.com/website-templates/responsive-bootstrap-theme-for-products-velocity/" target="_blank">
                                    <!-- <img class="img-responsive project-image" src="assets/images/projects/project-featured.png" alt="project name" /> -->
                                    </a>
                                    <div class="ribbon">
                                        <div class="text">Awesome</div>
                                        </div>
                                    </div>
                                    
                                <div class="desc text-left">                                    
                                    <p><%= @resume["capstones"]["description"] %></p>
                                </div><!--//desc-->    
                            
                        </div><!--//content-->  
                    </div><!--//section-inner-->                 
                </section><!--//section-->
                
                <section class="experience section">
                    <div class="section-inner">
                        <h2 class="heading">Work Experience</h2>
                        <div class="content">
                            <div class="item">
                                <h3 class="title"><%= @resume["experiences"]["job_title"] %> - <span class="place"><%= @resume["experiences"]["company"] %></span> <span class="year">(<%= @resume["experiences"]["start_date"] %> - <%= @resume["experiences"]["end_date"] %>)</span></h3>
                            </div><!--//item-->
                            
                        </div><!--//content-->  
                    </div><!--//section-inner-->                 
                </section><!--//section-->
                <section class="github section">
                    <div class="section-inner">
                        <h2 class="heading">Twitter Feed</h2>                     
                        <div id="ghfeed" class="ghfeed">
                        <p><%= @resume["twitter"] %></p>
                        </div><!--//ghfeed-->
                        
                    </div><!--//secton-inner-->
                </section><!--//section-->
            </div><!--//primary-->
            <div class="secondary col-md-4 col-sm-12 col-xs-12">
                 <aside class="info aside section">
                    <div class="section-inner">
                        <h2 class="heading sr-only">Basic Information</h2>
                        <div class="content">
                            <ul class="list-unstyled">
                                <li><i class="fa fa-map-marker"></i><span class="sr-only">Location:</span>Chicago, IL</li>
                                <li><i class="fa fa-envelope-o"></i><span class="sr-only"></span><a href="#">Email: <%= @resume["email"] %></a></li>                                
                                <li><i class="fa fa-link"></i><span class="sr-only"></span><a href="<%= @resume["blog_url"] %>">Blog</a></li>
                                <li><i class="fa fa-link"></i><span class="sr-only"></span><a href="<%= @resume["linkedin_url"] %>">LinkedIn</a></li>

                            </ul>
                        </div><!--//content-->  
                    </div><!--//section-inner-->                 
                </aside><!--//aside-->
                
                <aside class="skills aside section">
                    <div class="section-inner">
                        <h2 class="heading">Skills</h2>
                        <div class="content">
                            
                            <div class="skillset">
                               
                                <div class="item">
                                    <h3 class="level-title"><%= @resume["skills"]["skill_name"] %><span class="level-label" data-toggle="tooltip" data-placement="left" data-animation="true" title="You can use the tooltip to add more info...">Expert</span></h3>
                                    <div class="level-bar">
                                        <div class="level-bar-inner" data-level="96%">
                                        </div>                                      
                                    </div><!--//level-bar-->                                 
                                </div><!--//item-->

                            </div>              
                        </div><!--//content-->  
                    </div><!--//section-inner-->                 
                </aside><!--//section-->
                
                <aside class="education aside section">
                    <div class="section-inner">
                        <h2 class="heading">Education</h2>
                        <div class="content">
                            <div class="item">                      
                                <h3 class="title"><i class="fa fa-graduation-cap"></i><%= @resume["educations"]["degree"] %></h3>
                                <h4 class="university"><%= @resume["educations"]["school"] %> <span class="year">(<%= @resume["educations"]["start_date"] %>-<%= @resume["educations"]["end_date"] %>)</span></h4>
                            </div><!--//item-->
                        </div><!--//content-->
                    </div><!--//section-inner-->
                </aside><!--//section-->
              
            </div><!--//secondary-->    
        </div><!--//row-->
    </div><!--//masonry-->

